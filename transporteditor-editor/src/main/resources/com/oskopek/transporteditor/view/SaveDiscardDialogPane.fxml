<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.*?>
<DialogPane xmlns:fx="http://javafx.com/fxml/1" fx:id="dialogPane" prefWidth="500.0" xmlns="http://javafx.com/javafx/8"
            fx:controller="com.oskopek.transporteditor.controller.SaveDiscardController">
    <padding>
        <Insets bottom="10.0" left="10.0" right="10.0" top="10.0"/>
    </padding>
    <buttonTypes>
        <ButtonType text="%root.save" buttonData="YES"/>
        <ButtonType text="%root.saveAs" buttonData="APPLY"/>
        <ButtonType text="%root.discard" buttonData="NO"/>
        <ButtonType fx:constant="CANCEL"/>

    </buttonTypes>
</DialogPane>
